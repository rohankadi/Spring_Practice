<?xml version="1.0" encoding="UTF-8"?>
<!-- DTD(Documentation type data) file -->
<!-- DTD Changes for @Annotations -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />

	<bean class="com.kodnest.Spring_Project8.Driver" id="dr1">
		<property name="id" value="1"></property>
		<property name="name" value="Mohan"></property>
		<property name="age" value="30"></property>
	</bean>

<!-- @Autowired Internally make use of byType -->
	<bean class="com.kodnest.Spring_Project8.Car" id="car1"></bean>
</beans>